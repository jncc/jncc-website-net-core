@{
    Layout = null;
    //Html.RequiresJs("/js/lib/jquery-2.2.4.min.js", 1);
    //Html.RequiresJs("/js/lib/foundation-6.5.0.min.js");
}
<!doctype html>
<html @(await Component.InvokeAsync("PageAttributes", new { model = Model }))>
<head>
    <!-- #{Octopus.Action[deploy-website-to-iis].Package.PackageVersion} -->
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    @(await Component.InvokeAsync("SeoMetadata", new { model = Model }))
    <link rel="stylesheet" href="~/css/app.css?v1.2">
    <link rel="stylesheet" href="~/css/foundation-icons.css">
    <link rel="shortcut icon" href="~/favicon.ico" type="image/x-icon">
    @RenderSection("AdditionalHeadIncludes", false)
    <script src="/js/lib/foundation-6.5.0.min.js" defer></script>
    <script src="/js/lib/jquery-2.2.4.min.js"></script>
    <script src="https://ajax.aspnetcdn.com/ajax/jquery.validate/1.16.0/jquery.validate.min.js"></script>
    <script src="https://ajax.aspnetcdn.com/ajax/mvc/5.2.3/jquery.validate.unobtrusive.min.js"></script>

    <script src="https://cc.cdn.civiccomputing.com/9/cookieControl-9.x.min.js" type="text/javascript"></script>


    @(await Component.InvokeAsync("HeadIncludes", new { model = Model }))
</head>
<body>
    <noscript><div>Javascript must be enabled for the correct page display</div></noscript>
    <div id="wrapper">
        <a class="accessibility" href="#main" accesskey="S">Skip to Content</a>
        <header id="header">
            @(await Component.InvokeAsync("EditPageBar", new { model = Model }))
            @*@(Html.Action<HeaderSurfaceController>("RenderMemberLoggedInBar"))*@
            @RenderSection("Header")
        </header>
        @RenderBody()
        @RenderSection("Footer", false)
    </div>
    <a class="accessibility" href="#wrapper">Back to top</a>

    @RenderSection("EndOfBodyIncludes", false)

	<script src="~/js/app.js?v=1.3"></script>
	<script src="https://jncc.gov.uk/Cookie/Config/cookie-consent.js"></script>
	<link rel="stylesheet" href="~/css/cookie-consent.css">
    @(await Component.InvokeAsync("BodyIncludes", new { model = Model }))
</body>
</html>